#include <iostream>

using namespace std;

int maks(int a, int b, int c);

int main()

{

 int a,b,c;

 int max;

 cout<<"a= ";

 cin>>a;

 cout<<"b= ";

 cin>>b;

 cout<<"c= ";

 cin>>c;

 max=maks(a,b,c);

 cout<<"nilai maksimumnya adalah "<<max;

}

 int maks(int a, int b, int c)

{

 int max;

 max=a;

 if(max<b)

 {

 max=b;

 if(max<c)

 {

 max=c;

 }

 }

 else if (max<c)

 {

 max=c;

 }

 else

 {

 max=a;

 }

 return max;

}
